            <param name="value" value="banco.titular" />

            <param name="placeholder" value="msg:banco.titular.placeholder" />
            <param name="autocomplete" value="str:off" />
        </component>

        <component id="cuit" type="InputText">
            <param name="inputId" value="str:cuit" />
            <param name="name" value="str:cuit" />
            <param name="value" value="banco.cuit" />

            <param name="placeholder" value="msg:banco.cuit.placeholder" />
            <param name="autocomplete" value="str:off" />
        </component>

        <component id="cbu" type="InputText">
            <param name="inputId" value="str:cbu" />
