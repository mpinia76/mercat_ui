            <param name="value" value="banco.cbu" />

            <param name="placeholder" value="msg:banco.cbu.placeholder" />
            <param name="autocomplete" value="str:off" />
        </component>
        <component id="numero" type="InputText">
            <param name="inputId" value="str:numero" />
            <param name="name" value="str:numero" />
            <param name="value" value="banco.numero" />
            <param name="requiredMessage" value="msg:banco.numero.required" />
            <param name="placeholder" value="msg:banco.numero.placeholder" />
            <param name="autocomplete" value="str:off" />
        </component>

    <component id="saldoInicial" type="InputCurrency">
    		<param name="inputId" value="str:saldoInicial" />
    		<param name="name" value="str:saldoInicial" />
    		<param name="value" value="banco.saldoInicial" />
    		<param name="requiredMessage" value="msg:banco.saldoInicial.required" />
    		<param name="placeholder" value="msg:banco.saldoInicial" />
    		<param name="autocomplete" value="str:off" />
    	</component>








</rasty>
